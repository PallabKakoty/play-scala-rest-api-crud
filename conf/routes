GET        /                       controllers.HomeController.index

->         /v1/posts               v1.post.PostRouter

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file        controllers.Assets.at(path="/public", file)

POST       /api/create          controllers.HomeController.createProfile

POST       /api/retrieve        controllers.HomeController.getPerson

POST       /api/update          controllers.HomeController.updateNameCountry

POST       /api/delete          controllers.HomeController.deletePerson